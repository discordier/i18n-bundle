{
  "name": "cyberspectrum/i18n-bundle",
  "description": "Simple translation importing and exporting between various sources and destinations",
  "license": "MIT",
  "type": "symfony-bundle",
  "keywords": [
    "i18n"
  ],
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "https://www.cyberspectrum.de",
      "role": "Maintainer"
    }
  ],
  "homepage": "https://www.cyberspectrum.de/",
  "support": {
    "email": "info@cyberspectrum.de",
    "issues": "https://github.com/cyberspectrum/i18n-bundle/issues",
    "source": "https://github.com/cyberspectrum/i18n-bundle"
  },
  "require": {
    "php": "^8.1",
    "cyberspectrum/i18n": "^1.1@dev",
    "psr/log": "^2.0 || ^3.0",
    "symfony/config": "^5.4 || ^6.0 || ^7.0",
    "symfony/console": "^5.4 || ^6.0 || ^7.0",
    "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
    "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0",
    "symfony/yaml": "^5.4 || ^6.0 || ^7.0"
  },
  "require-dev": {
    "cyberspectrum/i18n-xliff": "^1.1@dev",
    "phpcq/runner-bootstrap": "^1.0@dev"
  },
  "suggest": {
    "cyberspectrum/i18n-contao": "For Contao content support",
    "cyberspectrum/i18n-metamodels": "For MetaModels support",
    "cyberspectrum/i18n-xliff": "For xliff support"
  },
  "autoload": {
    "psr-4": {
      "CyberSpectrum\\I18NBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CyberSpectrum\\I18NBundle\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-fix/phpcq-2": "1.1.x-dev"
    }
  }
}
